{
   "name": "ugc-template",
   "version": "0.0.6",
   "description": "This is a template for ugcplanet.",
   "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
   "homepage": "https://www.ugcplanet.com",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/bastianhilton/ugc-mockup"
   },
   "bugs": {
      "url": "https://github.com/bastianhilton/ugc-mockup/issues"
   },
   "license": "MIT",
   "private": true,
   "scripts": {
      "ugc-commit": "git add . && git commit -m \"ready for update\"",
      "ugc-update": "git pull \"https://github.com/bastianhiltonugc-mockup\"",
      "build": "nuxt build",
      "dev": "nuxt dev",
      "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "postinstall": "nuxt prepare"
   },
   "devDependencies": {
      "@nuxt/content": "^2.3.0",
      "@nuxtjs/apollo": "next",
