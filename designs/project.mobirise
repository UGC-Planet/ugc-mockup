        {
          "_name": "features03",
          "_sourceTheme": "healthm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features03 healthm5\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"12\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" name=\"showRating\" title=\"Rating\">\n        <input type=\"range\" inline title=\"Rating Amount\" name=\"rating\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showRating\">\n        <input type=\"checkbox\" name=\"showImage\" title=\"Card Image\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#ffffff\" name=\"imageBorder\" condition=\"showImage\" selected>\n        <input type=\"checkbox\" name=\"showName\" title=\"Card Name\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/healthm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/healthm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"items-wrapper\">\n                    <div class=\"item features-without-image\">\n                        <div class=\"item-wrapper\">\n                            <img src=\"@PROJECT_PATH@/assets/images/image4.jpg\" alt=\"Mobirise\">\n                            <div class=\"card-box\">\n                                <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".item-text\">\n                                    I noticed a significant improvement.\n                                </p>\n                                <div class=\"people-wrapper\" mbr-if=\"showImage || showRating || showName\">\n                                    <div class=\"people-wrap\">\n                                        <div class=\"image-wrap\" mbr-if=\"showImage\">\n                                            <img class=\"person\" src=\"@PROJECT_PATH@/assets/images/image17.jpg\" alt=\"Mobirise\">\n                                        </div>\n                                        <div class=\"rating-content\">\n                                            <div class=\"rating-wrapper\" mbr-if=\"showRating\">\n                                                <div class=\"rating-wrap\">\n                                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-rating, .rating-wrapper, .people-wrapper\">\n                                                        ★\n                                                    </p>\n                                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 1\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-rating, .rating-wrapper, .people-wrapper\">\n                                                        ★\n                                                    </p>\n                                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 2\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-rating, .rating-wrapper, .people-wrapper\">\n                                                        ★\n                                                    </p>\n                                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 3\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-rating, .rating-wrapper, .people-wrapper\">\n                                                        ★\n                                                    </p>\n                                                    <p class=\"mbr-rating mbr-fonts-style\" mbr-if=\"rating > 4\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-rating, .rating-wrapper, .people-wrapper\">\n                                                        ★\n                                                    </p>\n                                                </div>\n                                            </div>\n                                            <p class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showName\" data-app-selector=\".item-name, .people-wrapper\">\n                                                